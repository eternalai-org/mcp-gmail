{
  "name": "telegram-helper",
  "version": "1.0.0",
  "description": "TypeScript Node.js API with Express",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test-telegram": "node test-telegram.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build-docker": "docker build -t test .",
    "zip": "zip -r release.zip src package.json Dockerfile tsconfig.json greeting.txt",
    "chat": "ts-node src/chat.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@langchain/core": "^0.3.62",
    "@langchain/langgraph": "^0.3.6",
    "agent-server-definition": "^0.1.49",
    "ai": "^4.3.16",
    "axios": "^1.6.7",
    "composio-core": "^0.5.39",
    "cors": "^2.8.5",
    "cron": "^4.3.1",
    "dotenv": "^16.4.5",
    "ethers": "5.7.2",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.1.1",
    "winston": "^3.11.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
